{
  "RabbitMqSettings": {
    "HostName": "rabbitmq-prod",
    "Port": 5672,
    "OrderCreated": {
      "Queue": "oms-prod.order.created",
      "BatchSize": 200,
      "BatchTimeoutSeconds": 5,
      "DeadLetter": {
        "Dlx": "oms-prod.order.dlx",
        "Dlq": "oms-prod.order.created.dlq",
        "RoutingKey": "order.created"
      }
    },
    "OrderStatusChanged": {
      "Queue": "oms-prod.order.status.changed",
      "BatchSize": 200,
      "BatchTimeoutSeconds": 5,
      "DeadLetter": {
        "Dlx": "oms-prod.order.dlx",
        "Dlq": "oms-prod.order.status.changed.dlq",
        "RoutingKey": "order.status.changed"
      }
    }
  },
  "HttpClient": {
    "Oms": {
      "BaseAddress": "http://webapi-prod:8080"
    }
  }
}